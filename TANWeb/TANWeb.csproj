    <PackageReference Include="WindowsAzure.Storage" Version="9.3.3" />
  </ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\TAN.ApplicationCore\TAN.ApplicationCore.csproj" />
		<ProjectReference Include="..\TAN.DomainModels\TAN.DomainModels.csproj" />
		<ProjectReference Include="..\TAN.Repository\TAN.Repository.csproj" />

		<ProjectReference Include="..\TAN.DomainModels\TAN.DomainModels.csproj" />

	</ItemGroup>

	<ItemGroup>
		<Compile Update="Resources\TANResource.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>TANResource.resx</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Update="Resources\TANResource.resx">
			<Generator>ResXFileCodeGenerator</Generator>
			<LastGenOutput>TANResource.Designer.cs</LastGenOutput>
		</EmbeddedResource>
	</ItemGroup>

	<ItemGroup>
		<Folder Include="Areas\Inventory\Data\" />
		<Folder Include="Areas\LabourFileMover\Data\" />
		<Folder Include="Areas\LabourFileMover\Models\" />
		<Folder Include="Areas\PBJSnap\Data\" />
		<Folder Include="Areas\PBJSnap\Models\" />
		<Folder Include="Areas\TelecomReporting\Data\" />
		<Folder Include="Areas\TelecomReporting\Models\" />
	</ItemGroup>

	
<Choose>
		<When Condition="'$(Configuration)' == 'Production'">
			<ItemGroup>
				<None Include="appsettings.Production.json" CopyToOutputDirectory="Always" CopyToPublishDirectory="Always" />
				<None Include="appsettings.json" CopyToOutputDirectory="Never" CopyToPublishDirectory="Never" />
				<Content Remove="appsettings.*.json;" />
			</ItemGroup>
		</When>
		<When Condition="'$(Configuration)' == 'QA'">
			<ItemGroup>
				<None Include="appsettings.QA.json" CopyToOutputDirectory="Always" CopyToPublishDirectory="Always" />
				<None Include="appsettings.json" CopyToOutputDirectory="Never" CopyToPublishDirectory="Never" />
				<Content Remove="appsettings.*.json;" />
			</ItemGroup>
		</When>
		<When Condition="'$(Configuration)' == 'Staging'">
			<ItemGroup>
				<None Include="appsettings.Staging.json" CopyToOutputDirectory="Always" CopyToPublishDirectory="Always" />
				<None Include="appsettings.json" CopyToOutputDirectory="Never" CopyToPublishDirectory="Never" />
				<Content Remove="appsettings.*.json;" />
			</ItemGroup>
		</When>
		<Otherwise>
			<ItemGroup>
				<None Include="appsettings.json" CopyToOutputDirectory="Always" CopyToPublishDirectory="Always" />
				<None Include="appsettings.Development.json" CopyToOutputDirectory="Never" CopyToPublishDirectory="Never" />
			</ItemGroup>
		</Otherwise>
	</Choose>


	<ProjectExtensions><VisualStudio><UserProperties appsettings_1qa_1json__JsonSchema="https://json.schemastore.org/azure-iot-edge-deployment-template-4.0.json" /></VisualStudio></ProjectExtensions>
</Project>
