		<Configurations>Debug;Release;QA;Staging;Production</Configurations>
	</PropertyGroup>

	<ItemGroup>

		<None Include="Areas\PBJSnap\Views\Home\Index.cshtml" />

		<None Include="Areas\PBJSnap\Views\KronosHome\Index.cshtml" />

